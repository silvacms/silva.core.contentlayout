<div title="Components"
     i18n:domain="silva"
     i18n:attributes="title">
  <tal:block tal:repeat="block view.blocks">
    <h3><a href="#" tal:content="block.title">Title</a></h3>
    <div>
      <div class="component"
           tal:repeat="component block.components"
             tal:attributes="data-add component.add">
        <img src="${component.icon}" alt="${component.title}" />
        <span tal:content="component.title" />
      </div>
    </div>
  </tal:block>
</div>
