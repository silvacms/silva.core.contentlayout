<?xml version="1.0" encoding="utf-8"?>
<silva xmlns="http://infrae.com/namespace/silva"
	{namespaces}
	silva_version="{version}">
	<folder id="export">
		<metadata>
			<set id="silva-content">
				<silva-content:maintitle>Export base</silva-content:maintitle>
				<silva-content:shorttitle />
			</set>
			<set id="silva-extra">
				<silva-extra:comment />
				<silva-extra:contactemail />
				<silva-extra:contactname />
				<silva-extra:content_description />
				<silva-extra:creationtime>YYYY-MM-DDTHH:MM:SS
				</silva-extra:creationtime>
				<silva-extra:creator>author</silva-extra:creator>
				<silva-extra:keywords />
				<silva-extra:language />
				<silva-extra:lastauthor>author</silva-extra:lastauthor>
				<silva-extra:modificationtime>YYYY-MM-DDTHH:MM:SS
				</silva-extra:modificationtime>
				<silva-extra:subject />
			</set>
            <set xmlns:silvasettings="http://infrae.com/namespace/metadata/silva-settings" id="silva-settings">
              <silvasettings:hide_from_tocs>
                do not hide
              </silvasettings:hide_from_tocs>
            </set>
		</metadata>
		<content>
			<page id="page">
				<workflow>
					<version id="0">
						<status>unapproved</status>
						<publication_datetime />
						<expiration_datetime />
					</version>
				</workflow>
				<content version_id="0">
					<metadata>
						<set id="silva-content">
							<silva-content:maintitle>Page</silva-content:maintitle>
							<silva-content:shorttitle />
						</set>
						<set id="silva-extra">
							<silva-extra:comment />
							<silva-extra:contactemail />
							<silva-extra:contactname />
							<silva-extra:content_description />
							<silva-extra:creationtime>YYYY-MM-DDTHH:MM:SS
							</silva-extra:creationtime>
							<silva-extra:creator>author</silva-extra:creator>
							<silva-extra:keywords />
							<silva-extra:language />
							<silva-extra:lastauthor>author</silva-extra:lastauthor>
							<silva-extra:modificationtime>YYYY-MM-DDTHH:MM:SS
							</silva-extra:modificationtime>
							<silva-extra:subject />
						</set>
                        <set xmlns:silvasettings="http://infrae.com/namespace/metadata/silva-settings" id="silva-settings">
                          <silvasettings:hide_from_tocs>
                            do not hide
                          </silvasettings:hide_from_tocs>
                        </set>
					</metadata>
					<silva-core-contentlayout:design id="adesign">
						<silva-core-contentlayout:slot id="one">
							<silva-core-contentlayout:sourceblock id="cs_citation">
								<silva-external-sources:fields>
									<silva-external-sources:field id="citation">A joke is a very serious thing.</silva-external-sources:field>
									<silva-external-sources:field id="author">Winston Churchill</silva-external-sources:field>
									<silva-external-sources:field id="source" />
								</silva-external-sources:fields>
							</silva-core-contentlayout:sourceblock>
						</silva-core-contentlayout:slot>
					</silva-core-contentlayout:design>
				</content>
			</page>
		</content>
	</folder>
</silva>
