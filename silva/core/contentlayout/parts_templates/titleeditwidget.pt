<tal:init define="baseurl string:${context/absolute_url}/edit">
                  
<div class="header-title">Edit Page Title</div>
<form name="edit-dialog-form" id="edit-dialog-form" method="post"
 tal:attributes="action string:$baseurl/savepart">
<input type="hidden" name="parttype" value="page-title" />
<table width="100%" id="extsourceform" cellpadding="0" class="listing" cellspacing="0">
 <thead>
  <tr>
   <th>Element</th>
   <th>Value</th>
  </tr>
 </thead>
 <tbody>
   <tr class="even required">
    <td class="designator">
     <label for="page-title-edit-field">Page Title</label>
     <span class="warning">*</span>
    </td>
    <td>
     <input name="pagetitle" id="page-title-edit-field" class="store"
            tal:attributes="value context/get_title_editable" />
    </td>
   </tr>
 </tbody>
 </table>
 
</form>

</tal:init>